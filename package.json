{
  "name": "salmon-icons",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service scripts",
    "lint": "vue-cli-service lint",
    "build:icons": "node scripts/process.js",
    "copy-filled-icons": "node ./src/scripts/process-filled-icons.js",
    "gen-component-list": "node scripts/gen-component-list.js",
    "build:full": "webpack --config scripts/build/build.full.js",
    "build:components": "webpack --config scripts/build/build.components.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "rollup": "^2.56.2",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-vue": "^6.0.0",
    "stylus": "^0.54.7",
    "svgo": "^2.3.1",
    "typescript": "~4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "ignorePatterns": [
      "*.js"
    ],
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
