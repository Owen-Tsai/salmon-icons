{
  "name": "@salmon-ui/icons",
  "version": "1.1.1",
  "description": "A tree-shakeable Vue 3 icon library",
  "scripts": {
    "generate:sfc": "npx svgo -f ./icons -o ./icons && esno scripts/generate.ts",
    "build:sfc": "esno scripts/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run generate:sfc && npm run build:sfc && npm run build:types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Owen-Tsai/salmon-icons"
  },
  "keywords": [
    "vue",
    "icons"
  ],
  "author": "Owen",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "esbuild": "^0.14.39",
    "esbuild-plugin-globals": "^0.1.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "rollup": "^2.74.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "svgo": "^2.8.0",
    "typescript": "^4.6.4",
    "unplugin-vue": "^2.1.0",
    "vue": "^3.2.36",
    "vue-tsc": "^0.34.16"
  }
}
